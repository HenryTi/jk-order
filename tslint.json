{
  "extends": ["tslint-react"],
  "rules": {
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "jsx-alignment": false,
    "jsx-wrap-multiline": false,
    "react-tsx-curly-spacing": false,
    "ordered-imports": [false],
    "object-literal-sort-keys": [false]
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
